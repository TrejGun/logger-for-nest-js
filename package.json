    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-session": "1.18.0",
    "nest-winston": "1.10.0",
    "pg": "8.12.0",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0",
    "winston": "3.13.0",
