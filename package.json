    "@types/node": "14.14.13",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-ts": "0.4.2",
