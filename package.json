    "express-session": "1.17.3",
    "nest-winston": "1.7.0",
    "pg": "8.8.0",
    "rxjs": "7.5.7",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.10",
    "typeorm-naming-strategies": "4.1.0",
