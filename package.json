    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
