    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
